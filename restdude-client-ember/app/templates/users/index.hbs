

{{#if model.length}}
  <table>
    <thead>
      <tr>
        <th>
          Pk
        </th>
        <th>
          Username
        </th>
        <th>
          Firstname
        </th>
        <th>
          Lastname
        </th>
        <th>
          Email
        </th>
        <th>
          Password
        </th>
        <th>
          Passwordconfirmation
        </th>
        <th colspan="3">
          Actions
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |user|}}
        <tr>
          <td>
            {{user.pk}}
          </td>
          <td>
            {{user.username}}
          </td>
          <td>
            {{user.firstName}}
          </td>
          <td>
            {{user.lastName}}
          </td>
          <td>
            {{user.email}}
          </td>
          <td>
            {{user.password}}
          </td>
          <td>
            {{user.passwordConfirmation}}
          </td>
          <td>
            {{link-to "Edit" "users.edit" user}}
          </td>
          <td>
            {{link-to "Show" "users.show" user}}
          </td>
          <td>
            <a href="#" {{action "remove" user}}>Remove</a>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p id="blankslate">
    No Users found
  </p>
{{/if}}

<h1>Users list</h1>


{{title "Userzzzz" separator=">" prepend=false replace=false }}
<p>
  {{link-to "New User" "users.new" }}
</p>

{{#paper-list}}
  {{#paper-subheader}}3 line item{{/paper-subheader}}
  {{#each model as |user|}}
    {{#paper-item class="md-3-line"}}

     
      <div class="md-list-item-text">
      <h3>{{user.pk}}</h3>
      <h4>{{user.username}}</h4>
      <h4>{{user.firstName}}</h4>
      <h4>{{user.lastName}}</h4>
      <h4>{{user.email}}</h4>
      <h4>{{user.password}}</h4>
      <p>{{user.passwordConfirmation}}</p>
